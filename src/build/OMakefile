OCAMLINCLUDES[] +=
    ../libmojave
    ../front
    ../magic
    ../ir
    ../env
    ../exec
    ../shell
    ../eval

FILES[] =
    omake_rule
    omake_build_type
    omake_build_tee
    omake_build_util
    omake_builtin_type
    omake_target
    omake_builtin
    omake_build

